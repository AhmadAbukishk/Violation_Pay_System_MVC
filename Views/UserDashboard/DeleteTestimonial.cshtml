@model Traffic_Violation.Models.ProjectTestimonial

@{
    ViewData["Title"] = "DeleteTestimonial";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="row text-center pt-5">
    <h3>Are you sure you want to delete it?</h3>
    <div>    
        <form asp-action="TestimonialDeleted">
            <input type="hidden" asp-for="Testimonialid" />
            <input type="submit" value="Delete" class="btn btn-danger" /> 
            <a class="btn btn-primary" asp-action="TestimonialManagement">Back</a>
        </form>
    </div>
</div>

